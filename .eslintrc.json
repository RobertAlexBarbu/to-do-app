{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier" ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "lines-between-class-members": "off",
        "no-restricted-syntax": "off",
        "no-shadow": "off",
        "import/no-extraneous-dependencies": ["error", {"dependencies": true}]
    }
}
